package com.ustglobal.datastructure;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		 int n;
		 int arr[] = new int[n];
		
		System.out.println("Enter the element ");
		
		
		for(int i = 0 ; i<n ; i++) {
			arr[i] =sc.nextInt();
			
		}
		
		
		
		
		
		
	}
	
	
}
